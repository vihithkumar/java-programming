import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner reader = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = reader.nextInt();
int fact = 1;
        for(int i=1;i<=num;i++)
        {
            fact = fact *i;
        }
            System.out.println(fact);
    }
}
