import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner reader = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = reader.nextInt();

             for (int i=0;i<=num;i++)
             {
                 for(int j=0;j<i;j++)
                 {
                     System.out.print("*");
                 }

                 System.out.print("\n");


             }

    }
}
